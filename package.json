{
  "name": "tridentframe",
  "productName": "TridentFrame",
  "version": "0.1.0-beta.9",
  "description": "Desktop animated images swiss-army knife",
  "author": {
    "name": "StahlFerro",
    "email": "forgeworkseven@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "homepage": "https://stahlferro.netlify.app/softwares/tridentframe",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StahlFerro/TridentFrame.git"
  },
  "bugs": {
    "url": "https://github.com/StahlFerro/TridentFrame/issues"
  },
  "main": "main.js",
  "scripts": {
    "clean-build": "rimraf build/*",
    "clean-dist": "rimraf dist/*",
    "clean-cache": "rimraf temp/cache/*",
    "clean-previews": "rimraf temp/previews/*",
    "clean-engine": "rimraf engine/*",
    "clean-release": "rimraf release/*",
    "clean-mochawesome": "rimraf tests/javascript/mochawesome-report/*",
    "clean-all": "npm run clean-build && npm run clean-dist && npm run clean-engine && npm run clean-engine && npm run clean-cache && npm run clean-previews",
    "css-build": "sass --no-source-map sass/bulmamods.scss css/bulmamods.css",
    "css-watch": "sass --no-source-map --watch sass/bulmamods.scss css/bulmamods.css",
    "dev": "cross-env DEPLOY_ENV=DEV electron .",
    "prod": "electron .",
    "prod-windows": "npm run wpbuild && npm run buildpy-windows && npm run prod",
    "prod-linux": "npm run wpbuild && npm run buildpy-linux && npm run prod",
    "liverelease": "npm run clean-dist && webpack --watch",
    "wpserve": "webpack serve --mode development --env NODE_ENV=DEV",
    "wpbuild": "npm run clean-dist && webpack --mode production --env NODE_ENV=PROD",
    "elbuild": "npm run clean-release && electron-builder",
    "engine-clean": "rimraf ./engine/*",
    "buildpy-windows": "npm run engine-clean && pyinstaller tripy_windows64.spec",
    "buildpy-linux": "npm run engine-clean && pyinstaller tripy_linux.spec",
    "updatepy-windows": "npm run buildpy-windows && rimraf ./release/tridentframe/win-unpacked/resources/app/engine/ && xcopy engine release\\tridentframe\\win-unpacked\\resources\\app\\engine\\ /E",
    "updatepy-linux": "npm run buildpy-linux && rimraf ./release/tridentframe/linux-unpacked/resources/app/engine/ && cp -r ./engine/ ./release/tridentframe/linux-unpacked/resources/app/engine/",
    "release-windows": "npm run clean-build && npm run clean-cache && npm run clean-previews && npm run wpbuild && npm run buildpy-windows && npm run elbuild && npm run scribe-version",
    "release-linux": "npm run clean-build && npm run clean-cache && npm run clean-previews && npm run wpbuild && npm run buildpy-linux && npm run elbuild && npm run scribe-version",
    "scribe-version": "python writer/info_scribe.py",
    "test-py": "python -m pytest tests/python/unit/ tests/python/integration/ --html=tests/python/pytest-report/report.html --self-contained-html",
    "test-py-prod": "python -m pytest tests/python/production/ --html=tests/python/pytest-report/report.html --self-contained-html",
    "test-js-unit": "npm run clean-mochawesome && cross-env DEPLOY_ENV=DEV mocha './tests/javascript/unit/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-js-integration": "npm run clean-mochawesome && cross-env DEPLOY_ENV=DEV mocha './tests/javascript/integration/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-js": "npm run clean-mochawesome && cross-env DEPLOY_ENV=DEV mocha './tests/javascript/**/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-js-prod": "npm run clean-mochawesome && mocha './tests/javascript/**/*.js' --require @babel/polyfill --require @babel/register --exit",
    "test-all": "npm run test-py && npm run test-js && npm run test-py-prod && npm run test-js-prod"
  },
  "build": {
    "productName": "TridentFrame",
    "appId": "io.stahlferro.tridentframe",
    "directories": {
      "output": "release/tridentframe"
    },
    "asar": false,
    "win": {
      "target": [
        "7z",
        "zip"
      ],
      "icon": "release/tridentframe/win-unpacked/resources/app/imgs/TridentFrame_logo_256x256.ico"
    },
    "linux": {
      "target": [
        "7z",
        "deb",
        "tar.gz",
        "zip"
      ],
      "icon": "release/tridentframe/linux-unpacked/resources/app/imgs/TridentFrame_logo_256x256.png",
      "category": "Graphics"
    },
    "files": [
      "**/*",
      "engine/**",
      "dist/**",
      "!_DEVDOCS/**",
      "!.github/**",
      "!.sass-cache/**",
      "!.vscode/**",
      "!.venv/**",
      "!.mocharc.json",
      "!app.js",
      "!index.html",
      "!jsconfig.json",
      "!bin/**",
      "!config/**",
      "!css/**",
      "!pycore/**",
      "!node_modules/**",
      "!sass/**",
      "!temp/**",
      "!tests/**",
      "!venv/**",
      "!src/**",
      "!webfonts/**",
      "!writer/**",
      "!webpack.config.js",
      "!*.py",
      "!*.pylintrc",
      "!*.spec",
      "!*.vue",
      "!*.txt",
      "!*.code-workspace",
      "!*.ps1",
      "!*.sh",
      "!*.md",
      "!*.pytest_cache",
      "LICENSE.md",
      "!Pipfile",
      "!Pipfile.lock"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@popperjs/core": "^2.11.0",
    "mime-types": "^2.1.34",
    "python-shell": "^3.0.1",
    "vue-click-outside": "^1.1.0",
    "vue-fragment": "^1.5.2",
    "vue-slider-component": "^3.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.16.0",
    "babel-loader": "^8.2.3",
    "bulma": "^0.9.3",
    "bulma-slider": "^2.0.4",
    "copy-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "devtron": "^1.4.0",
    "electron": "^16.0.2",
    "electron-builder": "^22.14.5",
    "file-loader": "^6.2.0",
    "highlight.js": "^11.3.1",
    "html-webpack-plugin": "^5.5.0",
    "json-minimizer-webpack-plugin": "^3.1.1",
    "make-fetch-happen": "^9.1.0",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.1",
    "node-loader": "^2.0.0",
    "playwright": "^1.16.3",
    "rimraf": "^3.0.2",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "spectron": "^15.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dashboard": "^3.3.6",
    "webpack-dev-server": "^4.6.0",
    "wtfnode": "^0.9.1"
  }
}
